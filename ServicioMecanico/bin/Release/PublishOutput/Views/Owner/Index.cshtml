@model IEnumerable<ServicioMecanico.Models.Owner>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Id Propietario
        </th>
        <th>
            DNI
        </th>
        <th>
            Apellido
        </th>
        <th>
            Nombre
        </th>
        <th>
            Observaciones
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.IdOwner)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CardId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Observations)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.IdOwner  }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.IdOwner })
        </td>
    </tr>
}

</table>
